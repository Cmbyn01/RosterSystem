<div class="text-bg-primary p-3 text-center"><h1>Ops Effeciency</h1></div>






<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/">Performance Tech</a></li>
    <li class="breadcrumb-item"><a href="/B2B">B2B</a></li>
    <li class="breadcrumb-item active" aria-current="page">Ops Effeciency</li>

  </ol>
</nav>








<div class = "row">
  <div class = "col-6" ><h2>Upcoming Schedule</h2></div>
  <div class="col-6 d-flex justify-content-end">
<a href="/adduser2" type="button" class="btn btn-success align-self-center">+ Add Schedule</a>
  </div>
</div> 










<table class="table table-bordered">

  <thead class="thead-dark">
    <tr class="table-primary">
      <th scope="col">Week_Number</th>
      <th scope="col">Week_Date</th>
      <th scope="col">Primary_On-call</th>
      <th scope="col">email_1</th>
      <th scope="col">Secondary On-Call</th>
      <th scope="col">email_2</th>
      <th scope="col" class="text-end">Action</th>
    </tr>
  </thead>
  <tbody>
    
    {{#each rows}}
    <tr class="table-info">
      <td>{{this.Week_Number}}</td>
      <td>{{this.Week_Date}}</td>
      <td>{{this.Primary_Oncall}}</td>
      <td>{{this.Email}}</td>
      <td>{{this.Secondary_Oncall}}</td>
      <td>{{this.Email_2}}</td>
      <td class="text-end">
     <a href = "/edituser2/{{this.Week_Number}}" type = "button" class = "btn btn-light btn-sm"><i class = "bi bi-pencil"></i> Edit</a>
     <a href = "/ops/{{this.Week_Number}}" type = "button" class = "btn btn-light btn-sm"><i class = "bi bi-person-x"></i> Delete</a>

      </td>
    </tr>
    {{/each}}
   
    
  </tbody>
</table>